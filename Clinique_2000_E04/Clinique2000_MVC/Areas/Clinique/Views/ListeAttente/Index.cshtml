</p>

@foreach (var liste in Model)
{
    <div>
        <h4>ListeAttente</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.DateEffectivite)
            </dt>

            <dd>
                @Html.DisplayFor(model => liste.DateEffectivite)
            </dd>


        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = liste.ListeAttenteID }) |
